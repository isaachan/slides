          <mrow class="MJX-TeXAtom-ORD">
            <mi>c</mi>
            <mo>&#x22C5;<!-- ⋅ --></mo>
            <mi>t</mi>
          </mrow>
        </msup>
      </mstyle>
    </mrow>
