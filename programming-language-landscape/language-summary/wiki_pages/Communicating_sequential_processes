            <mi class="MJX-tex-mathit" mathvariant="italic">a</mi>
            <mi class="MJX-tex-mathit" mathvariant="italic">c</mi>
            <mi class="MJX-tex-mathit" mathvariant="italic">e</mi>
            <mi class="MJX-tex-mathit" mathvariant="italic">s</mi>
          </mrow>
        </mrow>
