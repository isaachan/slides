<span class="c"> 02014</span>        <span class="kr">SET</span> <span class="kp">DETAIL</span> <span class="nv">GROSS</span> <span class="o">=</span> <span class="kp">DETAIL</span> <span class="nv">GROSS</span> <span class="o">+</span> <span class="nv">MASTER</span> <span class="nv">RATE</span> <span class="o">*</span> <span class="mi">40</span><span class="p">,</span> <span class="nv">DO</span>
<span class="c"> 02015</span>      <span class="nv">FICA</span><span class="p">.</span><span class="nv">ROUTINE</span><span class="p">,</span> <span class="nv">DO</span> <span class="nv">WITHHOLDING</span><span class="p">.</span><span class="nv">TAX</span><span class="p">.</span><span class="nv">ROUTINE</span><span class="p">.</span>
 
<span class="c"> 02016</span>        <span class="kr">IF</span> <span class="nv">MASTER</span> <span class="nv">BONDEDUCT</span> <span class="kp">IS</span> <span class="ow">NOT</span> <span class="ow">EQUAL</span> <span class="kp">TO</span> <span class="no">ZERO</span> <span class="kr">THEN</span> <span class="nv">DO</span>
<span class="c"> 02017</span>      <span class="nv">BOND</span><span class="p">.</span><span class="nv">ROUTINE</span><span class="p">.</span>
 
<span class="c"> 02018</span>        <span class="nv">DO</span> <span class="kr">SEARCH</span> <span class="kp">FOR</span> <span class="kp">INDEX </span><span class="o">=</span> <span class="mi">1</span><span class="p">(</span><span class="mi">1</span><span class="p">)</span><span class="mi">12</span><span class="p">.</span>
 
<span class="c"> 02019</span> <span class="nv">NET</span><span class="p">.</span> <span class="kr">SET</span> <span class="nv">PAYRECORD</span> <span class="nv">NETPAY</span> <span class="o">=</span> <span class="kp">DETAIL</span> <span class="nv">GROSS</span> <span class="o">-</span> <span class="kp">DETAIL</span> <span class="nv">FICA</span> <span class="o">-</span> <span class="kp">DETAIL</span>
<span class="c"> 02020</span>        <span class="nv">WHT</span> <span class="o">-</span><span class="nv">DETAIL</span> <span class="nv">RETIREMENT</span> <span class="o">-</span> <span class="kp">DETAIL</span> <span class="nv">INSURANCE</span> <span class="o">-</span> <span class="kp">DETAIL</span>
<span class="c"> 02021</span>        <span class="nv">BONDEDUCT</span><span class="p">.</span>
</pre></div>
